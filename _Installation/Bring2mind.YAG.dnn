 <packages>
  <package name="Bring2mind_YAG" type="Module" version="01.00.00">
   <friendlyName>Yet Another Gallery</friendlyName>
   <description>Use Bring2mind's Yet Another Gallery module to display a set of pictures on your page.</description>
   <iconFile>~/DesktopModules/Bring2mind/YAG/images/yag_32.png</iconFile>
