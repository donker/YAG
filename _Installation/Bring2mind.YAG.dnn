   <releaseNotes/>
   <dependencies>
    <dependency type="CoreVersion">07.00.06</dependency>
   </dependencies>
   <components>
    <component type="Assembly">
     <assemblies>
      <assembly>
       <name>Bring2mind.DNN.Modules.YAG.dll</name>
       <sourceFileName>Bring2mind.DNN.Modules.YAG.dll</sourceFileName>
       <version>01.00.00</version>
      </assembly>
     </assemblies>
    </component>
    <component type="Module">
     <desktopModule>
      <moduleName>Bring2mind_YAG</moduleName>
      <foldername>Bring2mind\YAG</foldername>
      <moduleDefinitions>
       <moduleDefinition>
        <friendlyName>Yet Another Gallery</friendlyName>
        <defaultCacheTime>0</defaultCacheTime>
        <moduleControls>
         <moduleControl>
          <controlKey />
          <controlSrc>DesktopModules/Bring2mind/YAG/Gallery.ascx</controlSrc>
          <controlTitle>Gallery</controlTitle>
          <controlType>View</controlType>
         </moduleControl>
         <moduleControl>
          <controlKey>Edit</controlKey>
          <controlSrc>DesktopModules/Bring2mind/YAG/EditGallery.ascx</controlSrc>
          <supportsPartialRendering />
          <controlTitle>Edit Gallery</controlTitle>
          <controlType>Edit</controlType>
         </moduleControl>
         <moduleControl>
          <controlKey>Upload</controlKey>
          <controlSrc>DesktopModules/Bring2mind/YAG/Upload.ascx</controlSrc>
          <supportsPartialRendering />
          <controlTitle>Upload</controlTitle>
          <controlType>Edit</controlType>
         </moduleControl>
         <moduleControl>
          <controlKey>TemplateSettings</controlKey>
          <controlSrc>DesktopModules/Bring2mind/YAG/TemplateSettings.ascx</controlSrc>
          <supportsPartialRendering />
          <controlTitle>Edit Template Settings</controlTitle>
          <controlType>Edit</controlType>
         </moduleControl>
         <moduleControl>
          <controlKey>Settings</controlKey>
          <controlSrc>DesktopModules/Bring2mind/YAG/Settings.ascx</controlSrc>
          <supportsPartialRendering />
          <controlTitle>Simple Gallery Settings</controlTitle>
          <controlType>Edit</controlType>
         </moduleControl>
        </moduleControls>
        <permissions />
       </moduleDefinition>
      </moduleDefinitions>
     </desktopModule>
    </component>
    <component type="ResourceFile">
     <resourceFiles>
      <basePath>/DesktopModules/Bring2mind\YAG</basePath>
      <resourceFile>
       <name>resources.zip</name>
      </resourceFile>
     </resourceFiles>
    </component>
   </components>
  </package>
 </packages>
</dotnetnuke>